  "scripts": {
    "staging:start": "npm run clean && tsc && cross-env PROJECT_ENV=staging forever start ./dist/src/app.js",
    "daemon:server": "forever start server.js",
    "daemon:frontend": "forever start -c `which http-server` ./dist",
    "dev": "node server.js",
    "start": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "build": "node build/build.js",
    "list": "forever list",
    "daemon:server:stop": "forever stop server.js",
    "daemon:frontend:stop": "forever stop ./dist",
    "clean": "rm -rf ./dist",
    "migrate": "node ./server/migrations/migration.js",
    "rollback": "node ./server/migrations/user/user.rollback.js"
  },
